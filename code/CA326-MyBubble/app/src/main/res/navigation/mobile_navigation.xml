<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.CA326MyBubble.Layout_UI.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />


    <fragment
        android:id="@+id/navigation_info"
        android:name="com.CA326MyBubble.Layout_UI.info.InfoFragment"
        android:label="@string/title_info"
        tools:layout="@layout/fragment_info" />

    <fragment
        android:id="@+id/navigation_news"
        android:name="com.CA326MyBubble.Layout_UI.news.FragmentNews"
        android:label="@string/title_news"
        tools:layout="@layout/fragment_news" />

    <fragment
        android:id="@+id/navigation_bubble"
        android:name="com.CA326MyBubble.Layout_UI.bubble.BubbleFragment"
        android:label="@string/title_bubble"
        tools:layout="@layout/fragment_bubble" />

</navigation>